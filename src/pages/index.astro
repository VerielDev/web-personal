---
import AboutMeSection from "@/components/home/AboutMeSection.astro"
import ExperienceSection from "@/components/home/ExperienceSection.astro"
import FormContactSection from "@/components/home/FormContactSection.astro"
import HeroSection from "@/components/home/HeroSection.astro"
import ProjectsSection from "@/components/home/ProjectsSection.astro"
import SkillsSection from "@/components/home/SkillsSection.astro"
import TestimonialSection from "@/components/home/TestimonialSection.astro"
import { frontMatter } from "@/const/home"
import Layout from "@/layouts/Layout.astro"

const { title, description, canonical } = frontMatter
---

<Layout title={title} description={description} canonical={canonical}>
	<HeroSection />
	<SkillsSection />
	<ExperienceSection />
	<AboutMeSection />
	<ProjectsSection />
	<TestimonialSection />
	<FormContactSection />
</Layout>
