---
interface Props {
	textLevel6: string
}
const { textLevel6 } = Astro.props as Props
---

<div class="level-6">
	<p>Complejos</p>
	<div class="tooltip">
		<p set:html={textLevel6} />
	</div>
</div>
<style>
	.level-6 {
		position: absolute;
		cursor: pointer;
		top: 365px;
		right: 62%;
		border: 1px solid var(--color-zinc-100);
		padding: var(--p-20);
	}
</style>
